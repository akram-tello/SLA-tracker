{
  "name": "sla-tracker",
  "version": "1.0.0",
  "description": "SLA Tracker Dashboard - Monitor order processing performance across brands and countries",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "dotenv -e .env -- sh -c 'next start -p $PORT'",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:migrate": "mysql -u $ANALYTICS_DB_USER -p$ANALYTICS_DB_PASSWORD < migrations/001_create_schema.sql",
    "test": "echo \"No tests specified\" && exit 0",
    "etl:setup": "./scripts/setup-cron.sh",
    "etl:monitor": "./scripts/monitor-etl.sh",
    "etl:sync": "dotenv -e .env -- sh -c 'curl -X POST ${ETL_API_URL}/api/v1/etl/sync/'",
    "etl:status": "dotenv -e .env -- sh -c 'curl ${ETL_API_URL}/api/v1/etl/status/'",
    "etl:health": "dotenv -e .env -- sh -c 'curl ${ETL_API_URL}/api/v1/dashboard/health/'",
    "etl:cleanup": "dotenv -e .env -- sh -c 'curl ${ETL_API_URL}/api/v1/etl/cleanup/'",
    "etl:summary": "dotenv -e .env -- sh -c 'curl -X POST ${ETL_API_URL}/api/v1/etl/generate-summary/'",
    "user:create": "node scripts/create-user.js"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "apexcharts": "^4.0.0",
    "chart.js": "^4.4.6",
    "chartjs-plugin-annotation": "^3.1.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.36.3",
    "clsx": "^2.1.1",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.462.0",
    "luxon": "^3.5.0",
    "mysql2": "^3.11.4",
    "next": "15.1.6",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.1.0",
    "redis": "^4.7.0",
    "tailwind-merge": "^2.5.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^9.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.1.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "sla",
    "dashboard",
    "monitoring",
    "orders",
    "analytics",
    "nextjs",
    "mysql",
    "react"
  ],
  "author": "SLA Tracker",
  "license": "MIT"
}
